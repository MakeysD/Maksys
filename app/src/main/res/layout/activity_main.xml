<?xml version="1.0" encoding="utf-8"?>
<com.example.duan.chao.DCZ_selft.DragLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/dsl"
    android:background="@color/bg2"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="8.3"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="170dp">

            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rl1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/left">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdv1"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    fresco:placeholderImage="@mipmap/zhanwei"
                    fresco:placeholderImageScaleType="fitCenter"
                    fresco:roundedCornerRadius="5dp"
                    fresco:roundTopLeft="true"
                    fresco:roundTopRight="true"
                    fresco:roundBottomLeft="true"
                    fresco:roundBottomRight="true"/>
                <TextView
                    android:layout_toRightOf="@id/sdv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text01"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/text01"
                    android:text="安全保护"/>
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/you"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:layout_marginTop="1dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/left">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdv2"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    fresco:placeholderImage="@mipmap/zhanwei"
                    fresco:placeholderImageScaleType="fitCenter"
                    fresco:roundedCornerRadius="5dp"
                    fresco:roundTopLeft="true"
                    fresco:roundTopRight="true"
                    fresco:roundBottomLeft="true"
                    fresco:roundBottomRight="true"/>
                <TextView
                    android:layout_toRightOf="@id/sdv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text01"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/text01"
                    android:text="账户安全"/>
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/you"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:layout_marginTop="1dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/left">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdv3"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    fresco:placeholderImage="@mipmap/zhanwei"
                    fresco:placeholderImageScaleType="fitCenter"
                    fresco:roundedCornerRadius="5dp"
                    fresco:roundTopLeft="true"
                    fresco:roundTopRight="true"
                    fresco:roundBottomLeft="true"
                    fresco:roundBottomRight="true"/>
                <TextView
                    android:layout_toRightOf="@id/sdv3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text01"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/text01"
                    android:text="手势锁"/>
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/you"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:layout_marginTop="1dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/left">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdv4"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    fresco:placeholderImage="@mipmap/zhanwei"
                    fresco:placeholderImageScaleType="fitCenter"
                    fresco:roundedCornerRadius="5dp"
                    fresco:roundTopLeft="true"
                    fresco:roundTopRight="true"
                    fresco:roundBottomLeft="true"
                    fresco:roundBottomRight="true"/>
                <TextView
                    android:layout_toRightOf="@id/sdv4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text01"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/text01"
                    android:text="开启指纹解锁"/>
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/you"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:layout_marginTop="5dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/left">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdv5"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    fresco:placeholderImage="@mipmap/zhanwei"
                    fresco:placeholderImageScaleType="fitCenter"
                    fresco:roundedCornerRadius="5dp"
                    fresco:roundTopLeft="true"
                    fresco:roundTopRight="true"
                    fresco:roundBottomLeft="true"
                    fresco:roundBottomRight="true"/>
                <TextView
                    android:layout_toRightOf="@id/sdv5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text01"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/text01"
                    android:text="关于"/>
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/you"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:layout_marginTop="1dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/left">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdv6"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    fresco:placeholderImage="@mipmap/zhanwei"
                    fresco:placeholderImageScaleType="fitCenter"
                    fresco:roundedCornerRadius="5dp"
                    fresco:roundTopLeft="true"
                    fresco:roundTopRight="true"
                    fresco:roundBottomLeft="true"
                    fresco:roundBottomRight="true"/>
                <TextView
                    android:layout_toRightOf="@id/sdv6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text01"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/text01"
                    android:text="退出当前账户"/>
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/you"/>
            </RelativeLayout>
        </LinearLayout>
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.7">
        </View>
    </LinearLayout>
    <ScrollView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:paddingBottom="80dp"
        android:paddingTop="80dp"
        android:scrollbars="none">
    </ScrollView>
    <com.example.duan.chao.DCZ_selft.DragRelativeLayout
        android:id="@+id/rl_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/top"
                android:background="@color/bg1"/>
            <RelativeLayout
                style="@style/Title">
                <include
                    android:id="@+id/back"
                    layout="@layout/layout_back"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="账户安全管家"
                    android:textColor="@color/white"
                    android:textSize="@dimen/title" />
                <ImageView
                    android:id="@+id/scan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/right"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/saoma"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/xian"
                    android:background="@color/xian1"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:src="@mipmap/zhanwei"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:textColor="@color/colorAccent"
                    android:text="账户存在严重风险"/>
            </LinearLayout>
        </RelativeLayout>
    </com.example.duan.chao.DCZ_selft.DragRelativeLayout>

</com.example.duan.chao.DCZ_selft.DragLayout>
